/* pages/index/Studyyd/Studyyd.wxss */
@import "../../../../wxParse/wxParse.wxss";
.padd_lf{padding-left: 10rpx;}
.partyBranch{
  width: 100%;
  height: 100%;
}
.box{
  width: 100%;

  position: relative;
  overflow-x: hidden;
box-shadow: 0px 3px 10px 0px rgba(239, 240, 243, 0.99);
/* padding: 56rpx 30rpx 0; */
box-sizing: border-box;
}
.pos-top{
  width: 100%;
  position: fixed;
  
background: linear-gradient(60deg, #F35854, #FF7B79);
  left: 0;
  z-index: 10;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.beijing{
  width: 100%;
  height: 206rpx;
  left: 0;
  top: 0;
  position: absolute;
  z-index: -2;
}
.top-text{
  width: 100%;
  background: #fff;
  border-radius: 12rpx;
  padding: 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  align-content: space-between;
}
.text-shang{
  width: 100%;
  height: 190rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.nav .cu-item {
  height: 90rpx;
  display: inline-block;
  line-height: 90rpx;
  margin: 0 10rpx;
  padding: 0 20rpx;
  color: #333;
}

.s-left{
  width: 43%;
  height: 100%;
  background: red;
}
.s-right{
  width: 57%;
  height: 100%;
  padding: 5rpx 0 5rpx 20rpx;
box-sizing: border-box;
}
.s-right view:first-child{
  
font-size: 26rpx;
font-family: Microsoft YaHei;
font-weight: 400;
color: #333333;
width: 100%;
height: 20%;
}
.s-right view:last-child{
  padding-top: 10rpx;
width: 100%;
font-size: 22rpx;
font-family: Microsoft YaHei;
font-weight: 400;
color: #999999;
text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 4;
  line-clamp: 4;
  -webkit-box-orient: vertical;
}
.text-xia{
  width: 100%;
}
.x-top{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  font-size: 22rpx;
font-family: Microsoft YaHei;
font-weight: 400;
color: #666666;
margin-bottom: 10rpx;
}
.x-bot{
