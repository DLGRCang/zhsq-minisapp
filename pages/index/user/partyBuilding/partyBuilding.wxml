<import src="../../../wxParse/wxParse.wxml"></import>
<view class="partyBranch">
  <cu-custom bgColor="bg-gray-re" isBack="{{true}}">
    <view slot="content">社区党建</view>
  </cu-custom> 
  <scroll-view scroll-y="true" style="height:{{windowHeight}}px;" bindscrolltolower="cbott" bindscroll="scry">
    <view class="pos-top" wx:if="{{topPos}}">
    <scroll-view scroll-x class="nav" scroll-with-animation scroll-left="{{scrollLeft}}">
        <view class="cu-item {{index==TabCur?'text-huang cur':''}}" wx:for="{{dataTab}}" wx:key bindtap="tabSelect" data-id="{{index}}">
          {{item.content}}
        </view>
      </scroll-view>
    </view>
    <view>
  <view class="box">
    <image src="../../../../images/back2.png" class="beijing"></image>
    
    <view class="top-text" wx:if="{{rowsList.length != 0}}">
      <view class="text-shang">
        <view class="s-left"></view>
        <view class="s-right">
          <view>{{rowsList.name}}</view>
          <view>
            {{rowsList.introduce}}
          </view>
        </view>
      </view>
      <view class="text-xia">
        <view class="x-top">
          <view><text class="cuIcon-dianhua" style="color:#F35854"></text><text class="padd_lf">电话:{{rowsList.tel}}</text></view>
          <view><text class="cuIcon-homefill" style="color:#F35854"></text><text class="padd_lf">面积:{{rowsList.areaCovered}}㎡</text></view>
          <view><text class="cuIcon-timefill" style="color:#F35854"></text><text class="padd_lf">开放时间:{{rowsList.openDate}}</text></view>
          <view>
            <text class="cuIcon-locationfill" style="color:#F35854"></text><text class="padd_lf">地址:{{rowsList.address}}</text>
          </view>
        </view>

        
      </view>
    </view>
    <view class="fuwu">
      <view class="fuwunei" bindtap="dangqunfw">
        <image src="https://yiqi.sucstep.com/zhsq/assets/images/applets/love1.png"></image>
        <view style="width:100%">党群服务</view>
      </view>
      <view class="fuwunei" bindtap="changdifw">
        <image src="https://yiqi.sucstep.com/zhsq/assets/images/applets/dqfw2.png"></image>
        <view style="width:100%">场地服务</view>
      </view>
      <view class="fuwunei" bindtap="shequhd">
        <image src="https://yiqi.sucstep.com/zhsq/assets/images/applets/dqfw3.png"></image>
        <view style="width:100%">社区活动</view>
      </view>
      <view class="fuwunei" bindtap="studyyd">
        <image src="https://yiqi.sucstep.com/zhsq/assets/images/applets/dqfw4.png"></image>
        <view style="width:100%">学习园地</view>
      </view>
      <view class="fuwunei" bindtap="studysqdzb">
        <image src="https://yiqi.sucstep.com/zhsq/assets/images/applets/sqdzb.png"></image>
        <view style="width:100%">社区党支部</view>
      </view>
    </view>
    <view class="top-tab">
      <scroll-view scroll-x class="nav" scroll-with-animation scroll-left="{{scrollLeft}}">
        <view class="cu-item {{index==TabCur?'text-huang cur':''}}" wx:for="{{dataTab}}" wx:key bindtap="tabSelect" data-code="{{item.code}}" data-id="{{index}}">
          {{item.content}}
        </view>
      </scroll-view>
    </view>
  </view>
  <view class="bot-box" wx:if="{{TabCur == 0}}">
    <view class="queshiye" wx:if="{{rowsXw0.length == 0}}" style="padding-top:200rpx">
      <image src="https://yiqi.sucstep.com/zhsq/assets/images/applets/queshi.png"></image>
      <view>暂无数据</view>
    </view>
    <view class="xinwen" wx:for="{{rowsXw0}}" bindtap="xwDetails" data-item="{{item}}">
      <view class="seven_box">
        <view class="seven_left" style="{{item.head == ''?'width:100%':'width:75%'}}">
            <text>{{item.title}}</text>   
            <text>{{item.publicTime}}</text>  
        </view>
          <view class="seven_right" style="{{item.head == ''?'width:0':'width:25%'}}">
            <image src="http://172.16.20.81:9000/fileService/downloadFTP/public/{{item.head}}"></image>
          </view>
        </view>
      </view>
      <view class="cu-load {{!isLoad?'loading':'over'}}" wx:if="{{rowsXw0.length >= 12}}"></view>
  </view>
