    </view>
  </view>

  <view class="my-state">
    <view wx:for="{{t5state}}" wx:key="index" class="state-for" bindtap="orderClick" data-url="{{item.url}}">
      <image src="{{item.img}}"></image>
      <view>{{item.text}}</view>
    </view>
  </view>
