/* pages/index/feedback/feedback.wxss */
@import "../../../index/user/about/about.wxss";
.about>textarea{
  width: 100%;
height: 190px;padding:25rpx;
background: #F7F8FA;
border: 2px solid #E8EEF4;
border-radius: 12px;display: block;
}
.box-body{
  width: 100%;
  height: 100%;
  overflow-y: auto;
}
.beijing{
  width: 100%;
  height: 290rpx;
  left: 0;
  top: 0;
  position: absolute;
  z-index: -1;
}
.wx_timebox>.wx_title{line-height: 2;}
.wx_timebox>.wx_time{
  width: 100%;
background: #F7F8FA;
border: 2px solid #E8EEF4;
border-radius: 12rpx;
}
.wx_time>input{
  height: 90rpx;line-height: 90rpx;padding-left: 12rpx;
}
.about{text-indent: 0;font-size: 28rpx;}
.wx_timebox>textarea{
  width: 100%;
  background: #F7F8FA;
  border: 2px solid #E8EEF4;padding: 12rpx;
  border-radius: 12rpx;height:280rpx;
}
.tj_btn{
  width: 590rpx;margin-top:65rpx;margin-bottom: 45rpx;
  height: 87rpx;
  line-height: 87rpx;color: #fff;
  background: linear-gradient(60deg, #66B7F9, #3A91F7);
  border-radius: 44rpx;
}
