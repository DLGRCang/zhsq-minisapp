/* pages/index/complaint/complaint.wxss */
.complaint{
  width: 100%;
  height: 100%;
}
.lease-box{
  width: 100%;
  padding: 30rpx 30rpx 150rpx;
  box-sizing: border-box;
}
.box-body{
  width: 100%;
  background: #fff;
  border-radius: 12rpx;
  padding: 50rpx;
  box-sizing: border-box;
}
.body-top{
  width: 100%;
font-size: 30rpx;
font-family: Microsoft YaHei;
font-weight: bold;
color: #333333;
margin-bottom: 20rpx;
}
.danhang{
  width: 100%;
  padding: 15rpx 0;
}

.biaoti{
  font-size: 26rpx;
  font-family: Microsoft YaHei;
  color: #333333;
  margin-bottom: 20rpx;
  }
  .inputCss{
    height: 90rpx;
    background: #F7F8FA;
    border: 2rpx solid #E8EEF4;
    border-radius: 12rpx;
    }
    .btInput{
      width: 100%;
      height: 100%;
      font-size: 30rpx;
      padding: 0 24rpx;
      box-sizing: border-box;
    }
    .dhwbk{
      width: 100%;
      height: 180rpx;
    background: #F7F8FA;
    border: 2rpx solid #E8EEF4;
    border-radius: 12rpx;
    }
    .dhwbk textarea{
      width: 100%;
      height: 100%;
      padding: 20rpx;
      box-sizing: border-box;
      font-size: 26rpx;
    }
    .bot-fb{
      width: 100%;
      position: fixed;
      bottom: 0;
      left: 0;
      background: #fff;
      z-index: 10;
      height: 7%;
      display: flex;
      align-items: center;
      justify-content: center;
    }
    .bot-fb button{
      width: 200rpx;
    height: 70rpx;
    background: linear-gradient(60deg, #66B7F9, #3A91F7);
    border-radius: 30rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #fff;
    font-size: 26rpx;
    }