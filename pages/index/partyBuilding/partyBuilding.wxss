/* pages/index/partyBranch/partyBranch.wxss */
@import "../../../wxParse/wxParse.wxss";
.partyBranch{
  width: 100%;
  height: 100%;
}
.box{
  width: 100%;

  position: relative;
  overflow-x: hidden;
box-shadow: 0px 3px 10px 0px rgba(239, 240, 243, 0.99);
padding: 56rpx 30rpx 0;
box-sizing: border-box;
}
.pos-top{
  width: 100%;
  position: fixed;
  background: #f9f9f9;
  left: 0;
  z-index: 10;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.back-box1{
  width: 200%;
height: 488rpx;
background: #FA7D7B;
opacity: 0.3;
border-radius: 50%;
  position: absolute;
  top: -36%;
  left: -50%;
  z-index: -3;
}
.back-box2{
  position: absolute;
  top: -38%;
  left: -55%;
  z-index: -2;
  width: 200%;
height: 479rpx;
background: #FF7B79;
border-radius: 50%;
}
.back-box3{
  position: absolute;
  top: -40%;
  left: -50%;
  z-index: -1;
  width: 200%;
height: 479rpx;
  background: #F35854;
  border-radius: 50%;
}
.top-text{
  width: 100%;
  height: 370rpx;
  background: #fff;
  border-radius: 12rpx;
  padding: 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  align-content: space-between;
}
.text-shang{
  width: 100%;
  height: 70%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.s-left{
  width: 43%;
  height: 100%;
  background: red;
}
.s-right{
  width: 57%;
  height: 100%;
  padding: 5rpx 0 5rpx 20rpx;
box-sizing: border-box;
}
.s-right view:first-child{
  
font-size: 26rpx;
font-family: Microsoft YaHei;
font-weight: 400;
color: #333333;
width: 100%;
height: 20%;
}
.s-right view:last-child{
  padding-top: 10rpx;
width: 100%;
font-size: 20rpx;
font-family: Microsoft YaHei;
font-weight: 400;
color: #999999;
text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 5;
  line-clamp: 5;
  -webkit-box-orient: vertical;
}
.text-xia{
  width: 100%;
  height: 20%;

}
.x-top{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.x-top text:last-child{
  
font-size: 20rpx;
font-family: Microsoft YaHei;
font-weight: 400;
color: #666666;
}
.fuwu{
  width: 100%;
  height: 180rpx;
  background: #fff;
  margin-top: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-radius: 12rpx;
}
.fuwu .fuwunei{
  width: 33%;
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  align-content: center;
  text-align: center;
}
.fuwu image{
  width: 72rpx;
  height: 72rpx;
  margin: 0 auto;
  margin-bottom: 12rpx;
}
.top-tab{
  width: 100%;
}
.bot-box{
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}
.dang{
  width: 100%;

}
.neirong{
  width: 100%;
  background: #fff;
  padding: 25rpx 30rpx;
