}
.nrimg{
  width: 80rpx;
height: 80rpx;
background: #F55E5A;
border-radius: 50%;
margin-right: 20rpx;
}
.nrText{
  width: 70%;
  height: 80rpx;
  display: flex;
  flex-wrap: wrap;
  align-content: space-between;
}
.nrText view:first-child{
  width: 100%;
  
font-size: 26rpx;
font-family: Microsoft YaHei;
font-weight: 400;
color: #333333;
}
.nrText view:last-child{
  width: 100%;
  
font-size: 20rpx;
font-family: Microsoft YaHei;
