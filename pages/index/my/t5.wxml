<view class="t5" style="padding-bottom:40%">
  <view class="my-top" wx:if="{{loginJs}}">
    <view class="top-1">
      <view class="top-1-1"><image src="{{myLogin.avatarUrl}}"></image></view>
      <view class="top-1-2">
        <view>{{myLogin.nickName}}</view>
        <view>左右城左城</view>
      </view>
    </view>
    <image src="../../../images/t5/icon-right.png"></image>
  </view>
  <view class="my-top" >
    <view class="top-1" wx:if="{{!loginJs}}" bindtap="loginClick">
      <view class="top-1-1"></view>
      <view class="top-1-2">
        <view>请登录</view>
        <view>----</view>
      </view>
    </view>
    <view class="top_fr" bindtap="settingClick" >
      <image class="seeting_img" src="../../../images/t5/seeting.png"></image>
    </view>
  </view>

  <view class="my-state">
    <view wx:for="{{t5state}}" wx:key="index" class="state-for">
      <image src="{{item.img}}"></image>
      <view>{{item.text}}</view>
    </view>
  </view>
  <!-- 第一栏 -->
  <view class="my-b-from">
    <view class="my-p-from">
      <view class="bot-border">
        <text>我的预约</text>
        <image class="fromImg" src="{{rightHui}}"></image>
      </view>
  </view>
  <view class="my-p-from">
    <view class="bot-border">
        <text>我的投票</text>
        <image class="fromImg" src="{{rightHui}}"></image>
      </view>
  </view>
  <!-- <view class="my-from">
        <text>我的活动</text>
        <image class="fromImg" src="{{rightHui}}"></image>
  </view> -->
  </view>
  <!-- 第二栏 -->
  <view class="my-b-from">
    <view class="my-p-from">
      <view class="bot-border">
        <text>我的点赞</text>
        <image class="fromImg" src="{{rightHui}}"></image>
      </view>
  </view>
  <view class="my-p-from">
    <view class="bot-border">
        <text>我的收藏</text>
        <image class="fromImg" src="{{rightHui}}"></image>
      </view>
  </view>
  <view class="my-p-from">
    <view class="bot-border">
        <text>我的评论</text>
        <image class="fromImg" src="{{rightHui}}"></image>
      </view>
  </view>
  </view>

  <view class="my-b-from">
    <view class="my-p-from" bindtap="jfjlClick">
      <view class="bot-border">
        <text>缴费记录</text>
        <image class="fromImg" src="{{rightHui}}"></image>
      </view>
  </view>
  <view class="my-p-from" >
    <view class="bot-border" bindtap="wxjlClick">
        <text>维修记录</text>
        <image class="fromImg" src="{{rightHui}}"></image>
      </view>
  </view>
  </view>
  <view class="my-b-from">
    <view class="my-p-from">
      <view class="bot-border">
        <text>积分商城</text>
        <image class="fromImg" src="{{rightHui}}"></image>
      </view>
  </view>
  <view class="my-p-from">
    <view class="bot-border">
        <text>积分记录</text>
        <image class="fromImg" src="{{rightHui}}"></image>
      </view>
  </view>
  </view>

  <view class="my-b-from">
    <view class="my-p-from" bindtap="feedbackClick">
      <view class="bot-border">
        <text>帮助反馈</text>
        <image class="fromImg" src="{{rightHui}}"></image>
      </view>
  </view>
  <view class="my-p-from" bindtap="aboutMe">
    <view class="bot-border">
        <text>关于我们</text>
        <image class="fromImg" src="{{rightHui}}"></image>
      </view>
  </view>
  <view class="my-from">
        <text>清除缓存</text>
        <image class="fromImg" src="{{rightHui}}"></image>
  </view>
  </view>
</view>
